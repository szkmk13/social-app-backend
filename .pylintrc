[MASTER]
load-plugins=pylint_django, pylint_celery
ignore=migrations,tests,venv
django-settings-module=config.settings.test
[FORMAT]
max-line-length=120

[MESSAGES CONTROL]
disable=missing-docstring,invalid-name,astroid-error,unspecified-encoding

[DESIGN]
max-parents=13

[TYPECHECK]
generated-members=REQUEST,acl_users,aq_parent,"[a-zA-Z]+_set{1,2}",save,delete

[SIMILARITIES]
min-similarity-lines=6
